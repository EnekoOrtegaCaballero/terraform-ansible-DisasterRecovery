<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <connectionStrings>
    <add name="MyDbConn" 
         connectionString="Server=tcp:{{ db_host }},1433;Initial Catalog=master;Persist Security Info=False;User ID={{ db_user }};Password={{ db_password }};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=30;" 
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  
  <system.web>
    <customErrors mode="Off"/>
    <compilation debug="true"/>
  </system.web>
</configuration>
